#include <iostream>
using namespace std;
struct coordinate{
	float x,y;
};
int main() {
int count=0;
    coordinate arr[7];
    for (int i=0 ; i <7; i++){
    	cout << "Enter x corrdinate: ";
    	cin >> arr[i].x;
    	cout << "Enter y corrdinate: ";
    	cin >> arr[i].y;
    	if(arr[i].x>0 && arr[i].y>0)
    		count++;
    } 
    cout << "Number of coordinates in frst quadrant are: " << count << endl;
    return 0;
}
