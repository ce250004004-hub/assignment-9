#include <iostream>
using namespace std;
struct color{
	int red,blue,green;
};
int main() {
    struct color arr[5]={{100,200,67},{0,0,0},{150,50,100},{45,80,90},{1,2,3}};
    for(int i=0 ; i<3 ; i++){
    	arr[i].red=255-arr[i].red;
    	arr[i].blue=255-arr[i].blue;
    	arr[i].green=255-arr[i].green;
    }
    for (int i=0 ; i <5 ; i++){
    	cout << arr[i].red<<'\t'<<arr[i].blue<<'\t'<< arr[i].green<<endl;
    } 
    return 0;
}
